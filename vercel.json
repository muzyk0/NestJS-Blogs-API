{
  "version": 2,
  "name": "nest",
  "builds": [
    {
      "src": "apps/blogs-api/src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "apps/message-service/src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/blogs-api/src/main.ts"
    },
    {
      "src": "/(.*)",
      "dest": "apps/message-service/src/main.ts"
    }
  ]
}